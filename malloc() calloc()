#include <stdio.h>
#include <stdlib.h>
int main() {
 int *a,i;
 a=(int*)malloc(5*sizeof(int));
printf("Enter 5 elements");
for(i=0;i<5;i++)
{
    scanf("%d",(a+i));
}
printf("values are\n");
for(i=0;i<5;i++)
printf("%d\n",*(a+i));
}



#include <stdio.h>
#include <stdlib.h>
int main() {
 int *a,i;
 a=(int*)calloc(5,sizeof(int));
printf("Enter 5 elements");
for(i=0;i<5;i++)
{
    scanf("%d",(a+i));
}
printf("values are\n");
for(i=0;i<5;i++)
printf("%d\n",*(a+i));
}
